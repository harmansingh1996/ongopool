{
  "expo": {
    "$schema": "https://raw.githubusercontent.com/expo/expo/main/packages/@expo/config/schema/exp.json",
    "name": "OnGoPool",
    "slug": "ongopool-native",
    "scheme": "ongopool",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f172a"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.ongopool.app",
      "deploymentTarget": "15.1"
    },
    "android": {
      "package": "com.ongopool.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f172a"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "reactCompiler": false
    },
    "extra": {
      "eas": {
        "projectId": "changeme"
      }
    }
  }
}
